{"remainingRequest":"D:\\ninja-vue\\ninja-smoothies\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\ninja-vue\\ninja-smoothies\\src\\components\\AddSmoothie.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"D:\\ninja-vue\\ninja-smoothies\\src\\components\\AddSmoothie.vue","mtime":1580465294877},{"path":"D:\\ninja-vue\\ninja-smoothies\\node_modules\\css-loader\\dist\\cjs.js","mtime":1580444959628},{"path":"D:\\ninja-vue\\ninja-smoothies\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1580444971635},{"path":"D:\\ninja-vue\\ninja-smoothies\\node_modules\\postcss-loader\\src\\index.js","mtime":1580444967588},{"path":"D:\\ninja-vue\\ninja-smoothies\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1580444956338},{"path":"D:\\ninja-vue\\ninja-smoothies\\node_modules\\vue-loader\\lib\\index.js","mtime":1580444971634}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["AddSmoothie.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoHA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"AddSmoothie.vue","sourceRoot":"src/components","sourcesContent":["<!--Siyam-->\r\n<!--<template>-->\r\n<!--    <h2>Add Smoothie</h2>-->\r\n<!--</template>-->\r\n\r\n<!--<script>-->\r\n<!--    export default {-->\r\n<!--        name: \"AddSmoothie\",-->\r\n\r\n<!--        data() {-->\r\n<!--            return {-->\r\n\r\n<!--            }-->\r\n\r\n<!--        },//data-->\r\n<!--    }-->\r\n<!--</script>-->\r\n\r\n<!--<style scoped>-->\r\n\r\n<!--</style>-->\r\n<!--Siyam-->\r\n\r\n\r\n<!--Mostaq vai-->\r\n<template>\r\n    <div class=\"edit-smoothie container\">\r\n        <h2 class=\"center-align indigo-text\">Add New smoothie Recipe</h2>\r\n        <form @submit.prevent=\"AddSmoothie\">\r\n            <div class=\"field title\">\r\n                <label for=\"title\">Smoothie title</label>\r\n                <input type=\"text\" name=\"title\" v-model=\"title\">\r\n            </div>\r\n            <div v-for=\"(ing,index) in ingredients\" :key=\"index\" class=\"field add-ingredient\">\r\n                <label for=\"ingredient\">Add an ingredient</label>\r\n                <input type=\"text\" name=\"ingredient\" v-model=\"ingredients[index]\">\r\n                <i class=\"material-icons delete\" @click=\"deleteIng(ing)\">delete</i>\r\n            </div>\r\n            <div class=\"field add-ingredient\">\r\n                <label for=\"field add-ingredient\">Add an ingredient</label>\r\n                <input type=\"text\" name=\"add-ingredient\" @keydown.tab.prevent=\"AddMoreIngredient\" v-model=\"another\">\r\n            </div>\r\n            <div class=\"field center-align\">\r\n                <p v-if=\"feedback\" class=\"red-text\"> {{feedback}}</p>\r\n                <button class=\"btn pink\">Add Smoothie</button>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n       import db from '@/firebase/init'\r\n    import  slugify from 'slugify'\r\n\r\n    export default {\r\n        name: 'AddSmoothie',\r\n        data(){\r\n            return {\r\n                title: null,\r\n                slug: null,\r\n                another: '',\r\n                ingredients: [],\r\n                feedback: null\r\n            }\r\n        },\r\n        methods: {\r\n            AddSmoothie(){\r\n                // console.log(this.title, this.another, this.ingredients, this.feedback)\r\n                if(this.title){\r\n                    this.feedback = null;\r\n\r\n                    // create a slug\r\n                    this.slug = slugify(this.title, {\r\n                        replacement: '-',\r\n                        remove: /[$*_+~.()'\"!\\-:@]/g,\r\n                        lower: true\r\n                    });// slugify\r\n\r\n                    db.collection('smoothies').add({\r\n                        title: this.title,\r\n                        ingredients: this.ingredients,\r\n                        slug: this.slug\r\n                    }).then(() => {this.$router.push({name: 'Index'})}).catch(err => {\r\n                        console.log(err)\r\n                    })// catch\r\n                }// if\r\n                else {\r\n                    this.feedback = 'You must enter a value to add an ingredient'\r\n\r\n                }// else\r\n            },// AddSmoothie\r\n\r\n            AddMoreIngredient(){\r\n                if(this.another){\r\n                    // console.log('CHECK: ', this.ingredients[0]);\r\n                    this.ingredients.push(this.another);\r\n                    this.another = null\r\n                    this.feedback = null\r\n                }// if\r\n                else {\r\n                    this.feedback = 'You must enter a value to add an ingredient'\r\n                }// else\r\n            },// AddMoreIngredient\r\n\r\n            deleteIng(ing){\r\n                this.ingredients = this.ingredients.filter(ingredient => {\r\n                    return ingredient !== ing\r\n                })// filter\r\n\r\n            },// deleteIng\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .edit-smoothie {\r\n        margin-top: 60px;\r\n        padding: 20px;\r\n        max-width: 500px;\r\n    }\r\n\r\n    .edit-smoothie h2 {\r\n        font-size: 2em;\r\n        margin: 20px auto;\r\n    }\r\n\r\n    .edit-smoothie .field {\r\n        margin: 20px auto;\r\n        position: relative;\r\n    }\r\n    .edit-smoothie .delete{\r\n        position: absolute;\r\n        right: 0;\r\n        bottom: 16px;\r\n        color: #aaa;\r\n        font-size: 1.4em;\r\n        cursor: pointer;\r\n\r\n    }\r\n</style>\r\n\r\n"]}]}